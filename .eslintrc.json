{
  "extends": [
		"airbnb-base"
	],
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
	"plugins": [
		"@stylistic"
	],
  "rules": {
		"semi": ["error", "never"],
    "no-console": "off",
    "import/extensions": "off",
		"operator-linebreak": "off",
		"no-plusplus": "off",
		"no-shadow": "off",
		"import/prefer-default-export": "off",
		"operator-assignment": "off",
		"no-param-reassign": "off",
		"no-restricted-syntax": "off",
		"max-len": "off"
  }
}
